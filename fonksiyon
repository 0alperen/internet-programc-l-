function selamver(mesaj) {
    console.log(mesaj)

}

selamver("merhaba");
selamver("selam");
selamver("mesaj");


function yasHesapla(dogumYili){
    return new Date().getFullYear() - dogumYili
}

function emeklilikYasHesapla(dogumYili, isim) {
    let yas = yasHesapla(dogumYili)
    let kalanSene = 65 - yas
    if (kalanSene > 0 )
        console.log('Sn. $(isim) emekli olman覺za $(kalanSene) y覺l kalm覺st覺r')
    else 
        console.log("zaten emekli oldunuz")
}

emeklilikYasHesapla(1980, "ayse")
emeklilikYasHesapla(1961, "ahmet")
